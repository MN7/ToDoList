{
  "name": "to_do_list_api",
  "version": "1.0.0",
  "description": "To Do List app built with MERN stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix v00reactapp",
    "start": "node server.js",
    "server": "nodemon server.js",
    "kill-nodemon": "kill -9 $(ps aux | grep -i nodemon | awk '{print $2}')",
    "kill-portlistener": "kill -9 $(lsof -i tcp:5000 | awk '{print $2}')",
    "client": "npm start --prefix v00reactapp",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Narayan",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.16"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
